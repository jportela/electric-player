{
  "name": "electric-player",
  "version": "1.0.0",
  "description": "Desktop Music Player powered by Electron",
  "main": "dist/main.js",
  "scripts": {
    "build": "npm run build-webpack",
    "build-watch": "npm run build-webpack -- --watch",
    "build-webpack": "webpack --config webpack/webpack.config.js",
    "lint": "tslint -p tsconfig.json",
    "open": "electron .",
    "start": "concurrently \"npm:build-watch\" \"npm:open\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jportela/electric-player.git"
  },
  "keywords": [
    "electron",
    "music",
    "player"
  ],
  "author": "Jo√£o Portela",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jportela/electric-player/issues"
  },
  "homepage": "https://github.com/jportela/electric-player#readme",
  "devDependencies": {
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/styled-components": "^4.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^4.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "electron": "^3.0.7",
    "grommet": "^2.0.0-rc",
    "grommet-icons": "^3.2.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "styled-components": "^4.0.3",
    "ts-loader": "^5.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  }
}
